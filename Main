                    arsi[num] = 0;

                    ChangeTSI(form);

                    ChangeRCA(form);

                    return true;

                  }



                  msg = "Reuse Source Information field has invalid data: " + input.value + "\nField has been set back to 0. Please enter again.";



                  var str = input.value;

                  for (var i = 0; i < str.length; i++) {

                    var ch = str.substring(i, i + 1)

                    if ((ch < "0" || "9" < ch) && ch != '.') {

