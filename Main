                function CheckNumber(input, msg) {

                  msg = msg + " field has invalid data: " + input.value + "\nField will be cleared. Please enter again.";



                  var str = input.value;

                  for (var i = 0; i < str.length; i++) {

                    var ch = str.substring(i, i + 1)

                    if ((ch < "0" || "9" < ch) && ch != '.') {

                      alert(msg);

                      input.value = "";

                      input.focus()

                      return false;

                    }

                  }

                  return true;

                }



                // convert decimal to a string with only 2 digits after the decimal

                function dec_2(n) {

                  negFlag = 0

                  if (parseFloat(n) < 0) {

                    negFlag = 1

                  }

                  m = Math.abs(parseFloat(n))

                  x = parseInt("0" + m + "", 10)

                  y = (parseFloat(m) - x) * 100

                  z = Math.abs(parseInt("0" + y + "", 10))

                  zstr = "" + z + ""



                  // Format result...

                  ret = x + "." + zstr;

                  if (zstr.length == 1) { ret = x + "." + zstr + "0"; }

                  if (z == 0) { ret = x; }

                  if (negFlag == 1) {

                    ret = "-" + ret

                  }

                  return ret

                }



                function MakeArray(n) {

                  this.length = n

                  for (var i = 1; i <= n; i++)

                    this[i] = 0

                  return this

                }



