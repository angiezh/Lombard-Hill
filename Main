          <script language="JavaScript">

            function CheckNumber(input, msg) {
              msg = msg + " field has invalid data: " + input.value + "\nField will be cleared. Please enter again.";

              var str = input.value;
              for (var i = 0; i < str.length; i++) {
                var ch = str.substring(i, i + 1)
                if ((ch < "0" || "9" < ch) && ch != '.') {
                  alert(msg);
                  input.value = "";
                  input.focus()
                  return false;
                }
              }
              return true;
            }

            function dec_2(n) {
              negFlag = 0
              if (parseFloat(n) < 0) {
                negFlag = 1
              }
              m = Math.abs(parseFloat(n))
              x = parseInt("0" + m + "", 10)
              y = (parseFloat(m) - x) * 100
              z = Math.abs(parseInt("0" + y + "", 10))
              zstr = "" + z + ""

              // Format result...
              ret = x + "." + zstr;
              if (zstr.length == 1) { ret = x + "." + zstr + "0"; }
              if (z == 0) { ret = x; }
              if (negFlag == 1) {
                ret = "-" + ret
              }
              return ret
            }

            function ComputeForm(form) {
              // ensure all input fields have data
              if ((form.NEW.value == null || form.NEW.value.length == 0) ||
                (form.RSI.value == null || form.RSI.value.length == 0) ||
                (form.CostLOC.value == null || form.CostLOC.value.length == 0) ||
                (form.RCR.value == null || form.RCR.value.length == 0) ||
                (form.CostError.value == null || form.CostError.value.length == 0) ||
                (form.ErrorRate.value == null || form.ErrorRate.value.length == 0) ||
                (form.RCWR.value == null || form.RCWR.value.length == 0) ||
                (form.LOCWRO.value == null || form.LOCWRO.value.length == 0)) {
                form.TSI.value = "";
                form.RP.value = "";
                form.DCA.value = "";
                form.SCA.value = "";
                form.RCA.value = "";
                form.ADC.value = "";
                form.ROI.value = "";
                alert("There is at least one necessary field not filled in.")
              }
              else {
                var tmpTSI
                var tmpRP
                var tmpDCA
                var tmpSCA
                var tmpRCA
                var tmpADC
                var tmpROI

                // Calculate total source instructions

                tmpTSI = parseInt("0" + form.NEW.value, 10) + parseInt("0" + form.RSI.value, 10) + parseInt("0" + form.LOCWRO.value, 10)
                form.TSI.value = dec_2(tmpTSI)

                // Calculate reuse percentage
                tmpRP = parseInt("0" + form.RSI.value, 10) / parseInt("0" + form.TSI.value, 10) * 100
                form.RP.value = dec_2(tmpRP) + "%"

                // Calculate development cost avoidance
                tmpDCA = (1 - form.RCR.value) * form.CostLOC.value * form.RSI.value / 1000
                form.DCA.value = "$" + dec_2(tmpDCA) + "K"

                // Calculate service cost avoidance
                tmpSCA = parseInt(form.RSI.value, 10) / 1000 * parseFloat(form.ErrorRate.value) * parseFloat(form.CostError.value) / 1000
                form.SCA.value = "$" + dec_2(tmpSCA) + "K"


                // Calculate reuse cost avoidance
                tmpRCA = tmpDCA + tmpSCA
                form.RCA.value = "$" + dec_2(tmpRCA) + "K"

                // Calculate additional development cost
                tmpADC = parseFloat(form.CostLOC.value) * (parseFloat(form.RCWR.value) - 1) * parseInt("0" + form.LOCWRO.value, 10) / 1000
                form.ADC.value = "$" + dec_2(tmpADC) + "K"

                // Calculate ROI
                tmpROI = tmpRCA - tmpADC
                form.ROI.value = "$" + dec_2(tmpROI) + "K"

              }
            }

