                function ChangeROI(form) {

                  aroi[1] = arca[1] - aadc[1]

                  aroi[2] = arca[2] - aadc[2]

                  aroi[3] = arca[3] - aadc[3]

                  aroi[4] = arca[4] - aadc[4]

                  aroi[5] = arca[5] - aadc[5]

                  aroi[6] = arca[6] - aadc[6]

                  aroi[7] = arca[7] - aadc[7]

                  aroi[8] = arca[8] - aadc[8]

                  aroi[9] = arca[9] - aadc[9]

                  ChangeTROI(form);

                  DisplayRCA(form);

                  DisplayADC(form);

                  DisplayROI(form);

                }



                function ChangeADC(form) {

                  aadc[1] = alocwro[1] * (arcwr[1] - 1) * acostloc[1];

                  aadc[2] = alocwro[2] * (arcwr[2] - 1) * acostloc[2];

                  aadc[3] = alocwro[3] * (arcwr[3] - 1) * acostloc[3];

                  aadc[4] = alocwro[4] * (arcwr[4] - 1) * acostloc[4];

                  aadc[5] = alocwro[5] * (arcwr[5] - 1) * acostloc[5];

                  aadc[6] = alocwro[6] * (arcwr[6] - 1) * acostloc[6];

                  aadc[7] = alocwro[7] * (arcwr[7] - 1) * acostloc[7];

                  aadc[8] = alocwro[8] * (arcwr[8] - 1) * acostloc[8];

                  aadc[9] = alocwro[9] * (arcwr[9] - 1) * acostloc[9];

                  ChangeTADC(form);

                  ChangeROI(form);

                }



                function ChangeRCA(form) {

                  arca[1] = (arsi[1] * (1 - arcr[1]) * acostloc[1]) + (aerrorrate[1] * acosterror[1] * arsi[1] / 1000);

                  arca[2] = (arsi[2] * (1 - arcr[2]) * acostloc[2]) + (aerrorrate[2] * acosterror[2] * arsi[2] / 1000);

                  arca[3] = (arsi[3] * (1 - arcr[3]) * acostloc[3]) + (aerrorrate[3] * acosterror[3] * arsi[3] / 1000);

                  arca[4] = (arsi[4] * (1 - arcr[4]) * acostloc[4]) + (aerrorrate[4] * acosterror[4] * arsi[4] / 1000);

                  arca[5] = (arsi[5] * (1 - arcr[5]) * acostloc[5]) + (aerrorrate[5] * acosterror[5] * arsi[5] / 1000);

                  arca[6] = (arsi[6] * (1 - arcr[6]) * acostloc[6]) + (aerrorrate[6] * acosterror[6] * arsi[6] / 1000);

                  arca[7] = (arsi[7] * (1 - arcr[7]) * acostloc[7]) + (aerrorrate[7] * acosterror[7] * arsi[7] / 1000);

                  arca[8] = (arsi[8] * (1 - arcr[8]) * acostloc[8]) + (aerrorrate[8] * acosterror[8] * arsi[8] / 1000);

                  arca[9] = (arsi[9] * (1 - arcr[9]) * acostloc[9]) + (aerrorrate[9] * acosterror[9] * arsi[9] / 1000);

                  ChangeTRCA(form);

                  ChangeROI(form);

                }





                function ChangeRP(form) {

                  if (atsi[1] != "0") { arp[1] = dec_2(arsi[1] / atsi[1] * 100) }

                  else { arp[1] = "0" };



                  if (atsi[2] != "0") { arp[2] = dec_2(arsi[2] / atsi[2] * 100) }

                  else { arp[2] = "0" };



                  if (atsi[3] != "0") { arp[3] = dec_2(arsi[3] / atsi[3] * 100) }

                  else { arp[3] = "0" };



                  if (atsi[4] != "0") { arp[4] = dec_2(arsi[4] / atsi[4] * 100) }

                  else { arp[4] = "0" };



                  if (atsi[5] != "0") { arp[5] = dec_2(arsi[5] / atsi[5] * 100) }

                  else { arp[5] = "0" };



                  if (atsi[6] != "0") { arp[6] = dec_2(arsi[6] / atsi[6] * 100) }

                  else { arp[6] = "0" };



                  if (atsi[7] != "0") { arp[7] = dec_2(arsi[7] / atsi[7] * 100) }

                  else { arp[7] = "0" };



                  if (atsi[8] != "0") { arp[8] = dec_2(arsi[8] / atsi[8] * 100) }

                  else { arp[8] = "0" };



                  if (atsi[9] != "0") { arp[9] = dec_2(arsi[9] / atsi[9] * 100) }

                  else { arp[9] = "0" };

                  ChangeTRP(form);

                  DisplayTSI(form);

                  DisplayRP(form);

                }



                function ChangeTSI(form) {

                  PopulateArrays(form);

                  atsi[1] = arsi[1] + alocwro[1] + anewloc[1];

                  atsi[2] = arsi[2] + alocwro[2] + anewloc[2];

                  atsi[3] = arsi[3] + alocwro[3] + anewloc[3];

                  atsi[4] = arsi[4] + alocwro[4] + anewloc[4];

                  atsi[5] = arsi[5] + alocwro[5] + anewloc[5];

                  atsi[6] = arsi[6] + alocwro[6] + anewloc[6];

                  atsi[7] = arsi[7] + alocwro[7] + anewloc[7];

                  atsi[8] = arsi[8] + alocwro[8] + anewloc[8];

                  atsi[9] = arsi[9] + alocwro[9] + anewloc[9];

                  ChangeTRSI(form);

                  ChangeTLOCWRO(form);

                  ChangeTNEWLOC(form);

                  ChangeTTSI(form);

                  ChangeRP(form);



                }



                function ChangeRSI(form, input, num) {

