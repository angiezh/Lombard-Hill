                      input.focus();

                      arsi[num] = 0;

                      ChangeTSI(form);

                      ChangeRCA(form);

                      alert(msg);

                      return false;

                    }

                  }

                  arsi[num] = input.value;

                  ChangeTSI(form);

                  ChangeRCA(form);

                  return true;

                }



