                function ChangeCostLOC(form, input, num) {

                  PopulateArrays(form);

                  if (input.value == "") {

                    input.value = "100";

                    input.focus();

                    acostloc[num] = 100;

                    ChangeRCA(form)

                    ChangeADC(form)

                    return true;

                  }



                  msg = "New Development Cost field has invalid data: " + input.value + "\nField has been set back to 100. Please enter again.";



                  var str = input.value;

                  for (var i = 0; i < str.length; i++) {

                    var ch = str.substring(i, i + 1)

                    if ((ch < "0" || "9" < ch) && ch != '.') {

                      input.value = "100";

                      input.focus();

                      acostloc[num] = 100;

                      ChangeRCA(form)

                      ChangeADC(form)

                      alert(msg);

                      return false;

                    }

                  }

                  acostloc[num] = parseFloat(input.value)

                  ChangeRCA(form);

                  ChangeADC(form);

                  return true;

                }





